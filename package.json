{
  "name": "frame-by-frame-animation",
  "version": "0.1.0",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development webpack --watch",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "main": "src/main.js",
  "author": "kokoe",
  "license": "MIT",
  "homepage": "https://github.com/kokoe/frame-by-frame-animation#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:kokoe/frame-by-frame-animation.git"
  },
  "dependencies": {
    "bowser": "^2.9.0",
    "core-js": "^3.6.4",
    "lodash": "^4.17.19",
    "wolfy87-eventemitter": "^5.2.9"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "cross-env": "^7.0.0",
    "eslint": "^6.7.2",
    "regenerator-runtime": "^0.13.3",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "no-extra-boolean-cast": "off",
      "no-console": "process.env.NODE_ENV === 'production' ? 'error' : 'off'",
      "no-debugger": "process.env.NODE_ENV === 'production' ? 'error' : 'off'"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
